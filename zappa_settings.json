    "project_name": "deprepagos",
    "runtime": "python3.13",
    "timeout_seconds": 300,
    "memory_size": 1024,
    "s3_bucket": "faprivate",
    "keep_warm": true,
    "domain": "eventos.fuegoaustral.org",
