{
  "dev": {
    "alias": "dev",
    "slim_handler": true,
    "aws_region": "us-east-1",
    "django_settings": "deprepagos.settings_dev",
    "project_name": "deprepagos",
    "runtime": "python3.13",
    "timeout_seconds": 300,
    "memory_size": 1024,
    "s3_bucket": "faprivate",
    "keep_warm": true,
    "route53_enabled": false,
    "extra_permissions": [
      {
        "Effect": "Allow",
        "Action": [
          "s3:PutObject",
          "s3:GetObject",
          "s3:DeleteObject",
          "s3:ListBucket"
        ],
        "Resource": [
          "arn:aws:s3:::faprivate",
          "arn:aws:s3:::faprivate/*"
        ]
      }
    ],
    "domain": "dev.fuegoaustral.org",
    "certificate_arn": "arn:aws:acm:us-east-1:251799394474:certificate/09f6bc34-2d5a-4172-a25a-2602f1b15fca"
  },
  "prod": {
    "alias": "prod",
    "slim_handler": true,
